			正确的时机拍击时，也会让铃鼓重击敌怪
			{$Mods.ThoriumMod.Common.Tooltips.BigInstrument}
			'''
	}

	ColdShoulder: {
		DisplayName: 极寒镰刀
		Tooltip:
			'''
			无视10点防御力
			旋转一把极寒镰刀，攻击时造成霜冻
			不断喷出冰晶，造成50%伤害
			右键扔出镰刀，然后回到你身边
			"冷落你!"
			'''
	}

	CottonMouth: {
		DisplayName: 渊海镰刀
		Tooltip:
			'''
			无视20点防御力
			这把镰刀是定向摆动而不是旋转
			造成酸性毒液与硫磺海剧毒减益
			'''
	}

	DaedalusCowl: {
		DisplayName: 代达罗斯面罩
		Tooltip:
			'''
			增加10%光辉伤害
			增加5%治愈速度
			治疗法术会额外治疗3点生命值
			'''
