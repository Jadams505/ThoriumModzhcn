			# Weapons ==========================================================
			
			TestStick: {
				DisplayName: 棒球棍
				Tooltip: ""
			}

			AbyssFlower: {
				DisplayName: 黑渊白花
				Tooltip:
					'''
					以前文明纪元第六律者·死之律者的律者核心制成的神之键，集分解与创生的力量为一体。
					由表侧的纯白骑士枪和隐于里侧的黑色长枪组成,如黑渊之中仍会有白花绽放。
					这把武器真正的力量还在沉睡,但在未来某个时候,它将被唤醒。
					'''
			}

			Aphrodite: {
				DisplayName: 阿芙洛狄忒
				Tooltip:
					'''
					即使是在「诸神座」系列中，这把武器也实属异类——它收割敌人如此轻易，
					仿佛不是倚仗其锐利,而是凭美色倾倒众生，就像美神阿芙洛狄忒,自他人的鲜血中沐浴而生。
					'''
			}

			Balmung: {
				DisplayName: 巴鲁蒙格
				Tooltip:
					'''
					天命借助“魂钢”开发出的“诸神座”系列武器,
					刀身完全由崩坏能组成,可以切断一切,就算是传说中巨龙的鳞片也无法抵挡。
					'''
			}

			CrusherBunny19C: {
				DisplayName: 脆骨兔19C
				Tooltip: “兔兔那么可爱,为什么要吃兔兔?你就不怕被兔兔吃掉吗?嗯?”——「骇兔」布朗尼如是说。
			}

			EchoOfParadise: {
				DisplayName: 往世的华章
				Tooltip:
					'''
					一对被保存得颇为完好的老式燧发枪,
					红漆覆盖的枪身上镶嵌着银线花纹,黄铜制的枪管与火镰上残留着些许旧日的痕迹。
					即便是被陈列在一众精致的古玩与璀璨的宝石之中,它也显得毫不逊色。
					“你听,这燧石与火镰碰撞发出的声响，就像一个个美丽的音符，奏响了一曲华美的诗章。”
					'''
			}

			HekatesGloom: {
				DisplayName: 极夜之赫卡忒
				Tooltip:
					'''
					极为特殊的诸神座武器，和“努亚达”与“斯卡蒂”一样由神秘的异色魂钢块铸成。
					内置数量众多的小型飞刃“暮光”，十字架主体也能变成锋利的浮游巨剑,对敌人发起有如暴风雨的攻击。
					活性化的崩坏兽因子会使德丽莎的战场直觉变得异常敏锐,
					也正是在这种状态下,这件特殊设计的武器才得以展现出真正的力量。
					'''
			}

			HlersSerenity: {
				DisplayName: 永寂之赫勒尔
				Tooltip:
					'''
					特殊的诸神座武器，使用在大海深处中发现的异色魂钢块铸造而成。
					天命发现这块魂钢时，它正静静地漂流在黑暗而寂静的马里亚纳海沟中，宛如深海的住民。
					不知是这片死寂孕育了它,还是它造就了这片死寂…….
					'''
			}

			HyperRailguns: {
				DisplayName: 超电磁手炮
				Tooltip:
					'''
					ME社总部开发的手炮,利用9年前Project-MEI中的实验数据，
					成功将大量崩坏能粒子束缚在了电磁场中，并使用电磁场大幅强化了子弹。
					'''
			}

			IceEpiphyllum: {
				DisplayName: 仿灵刀·冰昙天
				Tooltip:
					'''
					天命圣1504研究所的所长，传说中活了上千年的刀工长光，在解析灵刀·樱吹雪的部分结构后，
					使用“魂钢”制作的仿制太刀，拥有强力的冰结之力。
					'''
			}

			KeyOfCastigation: {
				DisplayName: 天殛之钥
				Tooltip:
					'''
					伴随着雷之律者的觉醒,这把太刀也从雷鸣中显现。
					刀刃间迸裂出的闪电，臣服于雷电女王的力量之下。
					或许她的内心正如这一道道斩下的天殛，斩断了过去，也斩断了曾经的自己。
					'''
			}

			KeyOfEgo: {
				DisplayName: 真我之钥
				Tooltip:
					'''
					少女是如此深爱着这个世界——全心全意,毫无保留。
					她用她的箭，她的花，她的爱，唤醒这片枯萎的大地，在世界的尽头建造起那座无瑕的乐园。
					'''
			}

			KeyOfOblivion: {
				DisplayName: 无存之钥
				Tooltip:
					'''
					“老古董你为什么要用拳套？”
					“刀剑枪戈，皆是外物，若不能将自身之力磨砺至极，便不能说是真正的强大”
					“嗯，这话该让凯文听听。”
					'''
			}

			KeyOfRadiance: {
				DisplayName: 焢煌之钥
				Tooltip:
					'''
					斯火常燃于此.
					亘古长夜，如墨邃空，不曾让它暗灭；
					寒潮呼啸，冷风如刀，无法使它颤熄.
					它以人之意志为薪,意志不灭,柴薪亦不绝。
					现在，火焰传到了她的手中。		
					'''
			}

			KeyOfReason: {
				DisplayName: 真理之钥
				Tooltip:
					'''
					布洛妮娅使用理之律者的力量构造的火炮，
					不仅能装载在重装小兔双肩作为主武器使用，亦能独立在空中浮游援护主人。
					理之律者的构造之力原本需要日积月累的理解,
					但布洛妮娅能在短时间内构造出完成度如此高的火炮,或许是因为她本就对战场和武器无比熟悉。
					'''
			}

			KeyOfVoid: {
				DisplayName: 空无之钥
				Tooltip:
					'''
					复苏的第二律者从虚数空间中召唤出的异型武器，
					这对双枪射出的不是物理意义上的子弹,而是贯穿空间的“命令”。
					K423是奥托为律者的再次降临所准备的完美容器,
					这具身体甚至会对律者的力量产生影响,这也是空之律者的姿态发生了巨大改变的原因。
					'''
			}

			MightOfAnUtu: {
				DisplayName: 劫灭
				Tooltip:
					'''
					破坏之键在其原初持有者手中变化而成的异型大剑。
					宛如破坏神的化身降世，恒星入灭时爆发的光芒才能与其争辉，是谓“劫灭”之意的由来。
					唯有超越凡人的躯体,才能承受这无尽的毁灭力量。
					'''
			}

			NocturnalStealth: {
				DisplayName: 夜衔烛
				Tooltip:
					'''
					「天生剑胎」。早在少女尚不知「江湖」为何物之时,这一评断便已为她益刻下了人生的轨迹。
					千里求师，沙洲试剑,江湖诡谲,风雨飘摇。
					在这个名为「江湖」的熔炉中，少女与许多人相遇，亦与许多人作别。
					'''
			}

			OathOfJudah: {
				DisplayName: 犹大的誓约
				Tooltip:
					'''
					上一个时代文明留下的超神兵，拥有最强的“封印”之力，展开之后,在十字架附近的敌人会被禁锢，
					夺去其行动能力，并将其体内的崩坏能彻底无效化。
					'''
			}

			ObscuringWing: {
				DisplayName: 影夜栖羽
				Tooltip:
					'''
					寂静的暮色下，赤瞳的猎手栖身于交错的树影中。
					你永远也无法看清她的身影,因为当那黑翼掠过天幕,漆黑的利羽早已射向猎物的心脏。
					'''
			}

			RudderinDream: {
				DisplayName: 旅梦时舵
				Tooltip:
					'''
					某个诞生于拟似时间晶体中的人工智能,运用超越现代的材料技术打造而成的泛用型中近距离武器。
